        "Cyan": "#689d6a",
        "White": "#a89984",
        "BrightBlack": "#928374",
        "BrightRed": "#fb4934",
        "BrightGreen": "#b8bb26",
        "BrightYellow": "#fabd2f",
        "BrightBlue": "#83a598",
        "BrightMagenta": "#d3869b",
        "BrightCyan": "#8ec07c",
        "BrightWhite": "#ebdbb2",
        "Background": "#1d2021",
        "Foreground": "#ebdbb2",
        "SelectionBackground": "#665c54",
        "SelectionForeground": "#ebdbb2",
        "Transparent": "#ffffff00"
